{
  "name": "@repo/shared",
  "version": "0.0.1",
  "private": true,
  "description": "Shared types and utilities for create-fast-dev",
  "type": "module",
  "scripts": {
    "build": "unbuild",
    "watch": "unbuild --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vitest run --passWithNoTests",
    "clean": "rm -rf dist"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ]
}
